!URxvt.font: xft:FuraCode Nerd Font:style=Regular:size=10
!URxvt.boldfont: xft:FuraCode Nerd Font:style=Regular:size=10

! X colors.
! Generated by 'wal'
emacs*foreground:   #96c2d3
emacs*background:   #1b252a
URxvt*foreground:   #96c2d3
XTerm*foreground:   #96c2d3
UXTerm*foreground:  #96c2d3
URxvt*background:   [100]#1b252a
XTerm*background:   #1b252a
UXTerm*background:  #1b252a
URxvt*cursorColor:  #96c2d3
XTerm*cursorColor:  #96c2d3
UXTerm*cursorColor: #96c2d3
URxvt*borderColor:  [100]#1b252a

! Colors 0-15.
*.color0: #1b252a
*color0:  #1b252a
*.color1: #DB2A5B
*color1:  #DB2A5B
*.color2: #FB9E05
*color2:  #FB9E05
*.color3: #EDDA2C
*color3:  #EDDA2C
*.color4: #9AC048
*color4:  #9AC048
*.color5: #5457A6
*color5:  #5457A6
*.color6: #17979C
*color6:  #17979C
*.color7: #96c2d3
*color7:  #96c2d3
*.color8: #698793
*color8:  #698793
*.color9: #DB2A5B
*color9:  #DB2A5B
*.color10: #FB9E05
*color10:  #FB9E05
*.color11: #EDDA2C
*color11:  #EDDA2C
*.color12: #9AC048
*color12:  #9AC048
*.color13: #5457A6
*color13:  #5457A6
*.color14: #17979C
*color14:  #17979C
*.color15: #96c2d3
*color15:  #96c2d3

! Black color that will not be affected by bold highlighting.
*.color66: #1b252a
*color66:  #1b252a

! Xclock colors.
XClock*foreground: #96c2d3
XClock*background: #1b252a
XClock*majorColor:  rgba:96/c2/d3/ff
XClock*minorColor:  rgba:96/c2/d3/ff
XClock*hourColor:   rgba:96/c2/d3/ff
XClock*minuteColor: rgba:96/c2/d3/ff
XClock*secondColor: rgba:96/c2/d3/ff

! Urxvt configuration
URxvt*depth: 32
URxvt*scrollBar: false
